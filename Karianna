<script type="text/javascript">

        $("a.markerRVD").live("click", function (event) {

            event.preventDefault();

            $(this).toggleClass("opennode");

            var li = $(this).parentsUntil("li:first").parent();

            var liparent = $("li:first", li);

            var lichild = liparent.nextAll("li");

            lichild = lichild.add(liparent);

            if (lichild.is(":visible")) {

                if (!lichild.hasClass("noderoot")) {

                    $.each($("a", lichild), function () {

                        if ($(this).hasClass("opennode")) {
                            $(this).removeClass("opennode");
                        }
                    });

                    lichild.hide();
                    lichild.nextAll("li").hide();
                }

                $("li", lichild).hide();
            }
            else {
                lichild.show();
                lichild.nextAll("li").show();
            }
        });
        
        $(".checkcolor").click(function() {
            img = $(this).find("img");
            imgpath = img.attr("src");

            $("table").toggleClass("marks");
        
            if(imgpath.indexOf("_gray") != -1) {
                //color
                $(this).attr("title", "Журнал в цвете");
                img.attr("src", imgpath.replace("_gray", ""));	
                $("table").addClass("black");            
            } else {
                $(this).attr("title", "Черно-белый журнал");
                img.attr("src", imgpath.replace(".png", "_gray.png"));
                $("table").removeClass("black");	            
            }
        });



