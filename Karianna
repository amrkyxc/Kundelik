        document.addEventListener('readystatechange', function () {
            if (document.readyState === 'complete') {
                schoolDiaryMetrika.send('enter');
            }
        });
    });

    var analyticsAppButton = document.getElementById('TabRating');
    if (analyticsAppButton) {
        require(['schoolDiaryMetrika'], function (schoolDiaryMetrika) {
            schoolDiaryMetrika.send('viewRating');
            analyticsAppButton.addEventListener('click', function () {
                schoolDiaryMetrika.send('clickRating');
            });                                                 
        });   
